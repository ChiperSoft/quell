{
  "name": "quell",
  "version": "0.1.3",
  "description": "A no-frills active record implementation for node-mysql.",
  "main": "lib/quell.js",
  "scripts": {
    "test": "tap tests/unit",
    "test:cover": "tap --coverage tests/unit",
    "test:travis": "npm run lint && tap --coverage tests/unit",
    "lint": "eslint ./",
    "docs": "node docs/index.js"
  },
  "engines": {
    "node": ">=4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChiperSoft/quell.git"
  },
  "keywords": [
    "sql",
    "mysql",
    "query",
    "query-builder",
    "orm",
    "active-record",
    "activerecord"
  ],
  "author": "Jarvis Badgley <chiper@chipersoft.com> (http://chipersoft.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChiperSoft/quell/issues"
  },
  "homepage": "http://quelljs.com",
  "devDependencies": {
    "dox": "~0.9.0",
    "eslint": "~3.8.1",
    "eslint-config-chiper": "~2.0.1",
    "eslint-plugin-node": "~2.1.3",
    "eslint-plugin-promise": "~3.3.0",
    "handlebars": "~4.0.5",
    "tap": "~8.0.0",
    "tapsuite": "~1.0.1"
  },
  "dependencies": {
    "lodash.clone": "~4.5.0",
    "moment": "^2.5.1",
    "proxmis": "~1.0.0",
    "queryize": "^2.0.0"
  },
  "nyc": {
    "exclude": [
      "tests"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
